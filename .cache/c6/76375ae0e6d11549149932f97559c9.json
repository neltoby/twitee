":{"line":1,"column":1446}},{"source":"component/config/reducer/reducer.js","name":"draft","original":{"line":103,"column":16},"generated":{"line":1,"column":1450}},{"source":"component/config/reducer/reducer.js","name":"network","original":{"line":103,"column":22},"generated":{"line":1,"column":1452}},{"source":"component/config/reducer/reducer.js","name":"action","original":{"line":103,"column":32},"generated":{"line":1,"column":1460}},{"source":"component/config/reducer/reducer.js","name":"payload","original":{"line":103,"column":39},"generated":{"line":1,"column":1462}},{"source":"component/config/reducer/reducer.js","original":{"line":106,"column":8},"generated":{"line":1,"column":1472}},{"source":"component/config/reducer/reducer.js","name":"state","original":{"line":107,"column":15},"generated":{"line":1,"column":1480}},{"source":"component/config/reducer/reducer.js","name":"state","original":{"line":107,"column":15},"generated":{"line":1,"column":1487}},{"source":"component/config/reducer/reducer.js","original":{"line":16,"column":7},"generated":{"line":1,"column":1491}},{"source":"component/config/reducer/reducer.js","original":{"line":16,"column":7},"generated":{"line":1,"column":1499}},{"source":"component/config/reducer/reducer.js","original":{"line":16,"column":7},"generated":{"line":1,"column":1507}}],"sources":{"component/config/reducer/reducer.js":"import produce from 'immer'\r\nimport { \r\n    LOGGEDIN, \r\n    LOGGEDOUT, \r\n    USER_DETAILS, \r\n    ALL_POST, \r\n    LIKE_POST,\r\n    UNLIKE_POST,\r\n    COMMENT,\r\n    NETWORK_ERR,\r\n    NEW_POST,\r\n    LOAD_POST,\r\n    DELETE_POST\r\n} from '../actions'\r\n\r\nexport const reducer = (state, action) => {\r\n    switch(action.type){        \r\n        case LOGGEDIN: {\r\n            return produce(state, draft => {\r\n                draft.LoggedIn = LOGGEDIN\r\n            })\r\n        }\r\n        case LOGGEDOUT: {\r\n            return produce(state, draft => {\r\n                draft.LoggedIn = LOGGEDOUT\r\n            })\r\n        }\r\n        case USER_DETAILS: {\r\n            return produce(state, draft => {\r\n                draft.User = action.payload\r\n            })\r\n        }\r\n        case LOAD_POST: {\r\n            return produce(state, draft => {\r\n                draft.loadingPost = action.payload\r\n            })\r\n        }\r\n        case ALL_POST: {\r\n            return produce(state, draft => {\r\n                draft.Post = action.payload\r\n            })\r\n        }\r\n        case NEW_POST: {\r\n            return produce(state, draft => {\r\n                const newPosts = state.Post.slice()\r\n                draft.Post = [action.payload,...newPosts]\r\n            })\r\n        }\r\n        case LIKE_POST: {\r\n            return produce(state, draft => {\r\n                const { UserId, PostId } = action.payload\r\n                draft.Post = state.Post.map((item, i) => {\r\n                    if(item.id === PostId){\r\n                        const copyObj = JSON.parse(JSON.stringify(item))\r\n                        const likes = [...item.Likes, action.payload]\r\n                        copyObj.Likes = likes\r\n                        return copyObj\r\n                    }else{\r\n                        return item\r\n                    }\r\n                })\r\n            })\r\n        }\r\n        case UNLIKE_POST: {\r\n            return produce(state, draft => {\r\n                const { userId, postId } = action.payload\r\n                draft.Post = state.Post.map( item => {\r\n                    if(item.id === postId){\r\n                        const copyObj = JSON.parse(JSON.stringify(item))\r\n                        const likes = copyObj.Likes.filter(obj => obj.UserId !== userId)\r\n                        copyObj.Likes = likes\r\n                        return copyObj\r\n                    }else{ \r\n                        return item\r\n                    }\r\n                })\r\n            })\r\n        }\r\n        case DELETE_POST: {\r\n            return produce(state, draft => {\r\n                const { userId, postId } = action.payload\r\n                const newArr = state.Post.slice().filter(item => item.id !== postId)\r\n                draft.Post = newArr\r\n            })\r\n        }\r\n        case COMMENT: {\r\n            return produce(state, draft => {\r\n                const { PostId } = action.payload\r\n                draft.Post = state.Post.map(item => {\r\n                    if(item.id === PostId){\r\n                        const copyObj = JSON.parse(JSON.stringify(item))\r\n                        const comment = [action.payload,...copyObj.Comments]\r\n                        copyObj.Comments = comment \r\n                        return copyObj\r\n                    }else{\r\n                        return item\r\n                    }\r\n                })\r\n            })\r\n        }\r\n        case NETWORK_ERR: {\r\n            return produce(state, draft => {\r\n                draft.network = action.payload\r\n            })\r\n        }\r\n        default: \r\n        return state\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"0b97d622a18d11368b9574752c2c1396","cacheData":{"env":{}}}